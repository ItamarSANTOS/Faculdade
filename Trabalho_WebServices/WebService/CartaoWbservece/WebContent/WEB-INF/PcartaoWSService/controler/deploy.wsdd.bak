<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from PcartaoWSService WSDL service -->

  <service name="PcartaoWS" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://controler"/>
      <parameter name="wsdlServiceElement" value="PcartaoWSService"/>
      <parameter name="schemaQualified" value="http://controler,http://entities"/>
      <parameter name="wsdlServicePort" value="PcartaoWS"/>
      <parameter name="className" value="controler.PcartaoWSSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="PcartaoWS"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="find" qname="operNS:find" xmlns:operNS="http://controler" returnQName="retNS:findReturn" xmlns:retNS="http://controler" returnType="rtns:Pcartao" xmlns:rtns="http://entities" soapAction="" >
        <parameter qname="pns:saldo" xmlns:pns="http://controler" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="find"/>

      <typeMapping
        xmlns:ns="http://entities"
        qname="ns:Pcartao"
        type="java:entities.Pcartao"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
